{
  "name": "raidersdigest-all",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 8082",
    "proxy": "node proxy-server.js",
    "copy-public": "node copy-public-files.js",
    "build": "expo export --platform web && npm run copy-public",
    "build:prod": "NODE_ENV=production expo export --platform web && npm run copy-public"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@react-navigation/bottom-tabs": "^7.8.6",
    "@react-navigation/native": "^7.1.21",
    "@react-navigation/native-stack": "^7.8.0",
    "expo": "~54.0.25",
    "expo-constants": "~16.0.1",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.15",
    "expo-status-bar": "~3.0.8",
    "leaflet": "^1.9.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "~4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "^15.15.0",
    "react-native-web": "^0.21.0"
